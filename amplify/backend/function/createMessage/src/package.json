{
  "name": "lavapingcreatemessage",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.ts",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92"
  },
  "type": "commonjs",
  "scripts": {
    "build": "esbuild index.ts --bundle --minify --platform=node --target=es2020 --outfile=index.js",
    "zip:files": "zip -r function.zip index.js -x *.idea* *.git*",
    "deploy:function": "aws lambda update-function-code --profile development --function-name lavaping-development-proxy --zip-file fileb://function.zip"
  },
  "dependencies": {
    "esbuild": "^0.16.16",
    "node-fetch": "^3.3.1"
  }
}
